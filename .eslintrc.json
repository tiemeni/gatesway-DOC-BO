{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb", "prettier", "plugin:import/recommended"],
//   "plugins": "eslint-plugin-import",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "default-param-last": "off",
    "no-underscore-dangle": "off",
    "import/no-unresolved": [
      "error",
      {
        "ignore": ["^@iconscout/.+"]
      }
    ]
  }
}
